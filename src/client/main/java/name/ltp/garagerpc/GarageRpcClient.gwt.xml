<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.6.0//EN"
	"http://google-web-toolkit.googlecode.com/svn/tags/2.6.0/distro-source/core/src/gwt-module.dtd">
<module rename-to='GarageRpcClient'>
	<inherits name='com.google.gwt.user.User'/>
	<inherits name='com.google.gwt.query.Query'/>
	<inherits name='org.fusesource.restygwt.RestyGWT'/>
	<inherits name='name.ltp.garagerpc.GarageRpcShared'/>

	<generate-with class="name.ltp.garagerpc.rebind.RpcGenerator">
		<when-type-assignable class="name.ltp.garagerpc.shared.RpcService"/>
	</generate-with>

	<source path='client'/>
</module>
